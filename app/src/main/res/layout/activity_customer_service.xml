<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <LinearLayout
        android:id="@+id/linearLayout14"
        android:layout_width="0dp"
        android:layout_height="50dp"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <Button
            android:id="@+id/back_button1"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:background="@drawable/ic_back_signiture" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout13"
        android:layout_width="0dp"
        android:layout_height="168dp"
        android:layout_marginStart="1dp"
        android:layout_marginEnd="1dp"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout14">

        <LinearLayout
            android:layout_width="146dp"
            android:layout_height="161dp"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/imageView8"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                tools:srcCompat="@tools:sample/avatars" />
        </LinearLayout>

        <TextView
            android:id="@+id/textView7"
            android:layout_width="wrap_content"
            android:layout_height="95dp"
            android:layout_weight="1"
            android:background="#C3B9AB"
            android:text="안녕하세요 함께의 고객센터입니다. 불편하신 점이 있으시 다면 아래로 연락을 해 주시길 바랍니다. " />


    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout19"
        android:layout_width="0dp"
        android:layout_height="75dp"
        android:layout_marginTop="528dp"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout13"></LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout16"
        android:layout_width="0dp"
        android:layout_height="137dp"
        android:layout_marginStart="1dp"
        android:layout_marginTop="30dp"
        android:layout_marginEnd="1dp"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout13">

        <TableLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TableRow>

                <TextView
                    android:layout_width="114dp"
                    android:layout_height="50dp"
                    android:background="#90C9F6"
                    android:padding="3dip"
                    android:text="1" />

                <TextView
                    android:layout_width="251dp"
                    android:layout_height="match_parent"
                    android:gravity="left"
                    android:padding="3dip"
                    android:text="2" />
            </TableRow>

            <TableRow>

                <TextView
                    android:layout_width="134dp"
                    android:layout_height="50dp"
                    android:background="#90C9F6"
                    android:padding="3dip"
                    android:text="3" />

                <TextView
                    android:layout_width="216dp"
                    android:layout_height="match_parent"
                    android:gravity="left"
                    android:padding="3dip"
                    android:text="4" />
            </TableRow>
        </TableLayout>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout20"
        android:layout_width="0dp"
        android:layout_height="158dp"
        android:layout_marginStart="1dp"
        android:layout_marginTop="30dp"
        android:layout_marginEnd="1dp"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout16">

        <TableLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TableRow>

                <TextView
                    android:layout_width="114dp"
                    android:layout_height="50dp"
                    android:background="#90C9F6"
                    android:padding="3dip"
                    android:text="1" />

                <TextView
                    android:layout_width="251dp"
                    android:layout_height="match_parent"
                    android:gravity="left"
                    android:padding="3dip"
                    android:text="2" />
            </TableRow>

            <TableRow>

                <TextView
                    android:layout_width="134dp"
                    android:layout_height="50dp"
                    android:background="#90C9F6"
                    android:padding="3dip"
                    android:text="3" />

                <TextView
                    android:layout_width="216dp"
                    android:layout_height="match_parent"
                    android:gravity="left"
                    android:padding="3dip"
                    android:text="4" />
            </TableRow>
        </TableLayout>
    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>